{
  "name": "demo-bay",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "cy:open": "cypress open",
    "cy:component": "cypress run --component",
    "cy:component-coverage": "cypress run --component --coverage",
    "cy:e2e": "cypress run --e2e",
    "jest": "jest --no-cache",
    "jest:coverage": "jest --coverage",
    "test": "npm-run-all --parallel jest cy:component",
    "e2e": "start-server-and-test 'start' 4200 'cy:e2e'",
    "coverage": "npm-run-all jest:coverage",
    "analyze": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/demo-bay/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular/cli": "~15.2.4",
    "@angular/compiler-cli": "^15.2.0",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/cypress": "^9.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.5",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.8.1",
    "jest": "^29.4.3",
    "jest-preset-angular": "^13.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.2.7",
    "typescript": "~4.9.4"
  }
}
